   "source": [
    "#настройка соединения с DB\n",
    "\n",
    "import psycopg2\n",
    "conn = psycopg2.connect(dbname = \"postgres\", user = \"postgres\", host = \"localhost\", password = \"password\", port = \"5432\")"
